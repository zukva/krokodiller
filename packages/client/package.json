    "axios": "^1.2.0",
    "dotenv": "^16.0.2",
    "history": "5.3.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "4.18.2",
    "formik": "^2.2.9",
    "lodash": "4.17.21",
    "prettier": "^2.7.1",
    "qs": "6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
